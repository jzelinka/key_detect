\begin{frame}
   \frametitle{Traversability Assessment}
   \begin{columns}[t, onlytextwidth]
      \begin{column}[T]{0.5\textwidth}
         \begin{itemize}
            \item Assign value of \textbf{cost} to difficulty of traversing given terrain
            \item Used to \textbf{improve future path-planning} decisions
            \item Approaches:
            \begin{itemize}
               \item Values of cost described as
               \begin{itemize}
                  \item Discrete classifier
                  \item Continuous score
               \end{itemize}
               \item Sensing
               \begin{itemize}
                  \item Exteroceptive (LiDAR, camera)
                  \item Proprioceptive (sensors measuring internal properties)
               \end{itemize}
            \end{itemize}
         \end{itemize}
      \end{column}
      \begin{column}[T]{0.49\textwidth}
         \begin{figure}
            \begin{center}
               \includegraphics[width=\linewidth]{crop_em.jpg}
            \end{center}
         \end{figure}
      \end{column}
   \end{columns}
\end{frame}

% - frame -----------------------------------------------------------------
\begin{frame}
   \frametitle{Transfer Learning}
   \begin{columns}[t, onlytextwidth]
      \begin{column}[T]{0.44\textwidth}
         \begin{itemize}
            \item \textbf{Exchange of knowledge} between two domains
            \item \textbf{Difference} in perception or build yields the need for \textbf{heterogeneous} transfer learning
            % \item \textbf{Different} domains = transfer learning in \textbf{heterogeneous} team
            \item\textbf{Benefits:}
            \begin{itemize}
               \item Improve performance on given task thanks to transfered knowledge
               \item Reduce costly data collection
            \end{itemize}
         \end{itemize}
      \end{column}
      \begin{column}[T]{0.55\textwidth}
         \begin{figure}
            \begin{center}
               \includegraphics[width=\linewidth]{pres-schema-crop.pdf}
            \end{center}
            \parbox{\textwidth}{\tiny {\color{ctu4blue}J. Zelinka, M. Pr√°gr, R. Szadkowski, J. Bayer, J. Faigl}: \textit{Traversability Transfer Learning Between Robots with Different Cost Assessment Policies}, {\color{ctu4blue}MESAS}, 2021.}
         \end{figure}
      \end{column}
   \end{columns}
\end{frame}

